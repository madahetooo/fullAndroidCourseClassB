<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.apps.fullcourseandroidclassb">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:name=".breakingbadapp.ui.BreakingBadApplication"
        android:allowBackup="true"
        android:icon="@drawable/logocar"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logocar"
        android:supportsRtl="true"
        android:theme="@style/Theme.FullCourseAndroidClassB">
        <activity android:name=".breakingbadapp.ui.BreakingBad" />

        <service
            android:name="com.apps.fullcourseandroidclassb.pushnotification.service.FirebaseService"
            android:permission="com.google.android.c2dm.permission.SEND"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>

        <activity android:name=".clothesapp.ui.ClothesApp" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.ui.base.SplashScreen" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.utils.CoroutinesExamples" />
        <activity android:name=".shoppinglist.ui.ShoppingActivity" />
        <activity android:name=".onboardingscreen.ui.OnBoardingScreens" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.utils.BroadcastReceiverExample" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.utils.NotificationExample" /> <!-- <activity android:name=".ui.BullsCarsActivity" /> -->
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.ui.base.HomeActivity" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.ui.base.BaseActivity" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.ui.loginsystem.RegistrationActivity" />
        <activity android:name="com.apps.fullcourseandroidclassb.otherapps.ui.loginsystem.LoginActivity" /> <!-- <activity android:name=".ui.TodoListApplication" /> -->
        <activity
            android:name="com.apps.fullcourseandroidclassb.firebasefirestoreapp.ui.MainActivity"
            tools:ignore="Instantiatable" /> <!-- <activity android:name=".ResturantMenu" /> -->
        <activity
            android:name="com.apps.fullcourseandroidclassb.otherapps.ui.others.ImageViewExample"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.apps.fullcourseandroidclassb.otherapps.ui.others.Calculator"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.apps.fullcourseandroidclassb.otherapps.ui.others.Counter"
            tools:ignore="Instantiatable" />
        <activity android:name=".pushnotification.ui.CloudMessagingApp"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>